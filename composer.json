{
  "name": "benjaminmedia/wp-site-manager",
  "description": "Integration Plugin for Site Manager",
  "type": "wordpress-plugin",
  "keywords": ["wordpress", "plugin"],
  "homepage": "https://github.com/BenjaminMedia/wp-site-manager",
  "authors": [
    {
      "name": "Bonnier Publications",
      "email": "interactive@bonnier.dk",
      "homepage": "http://bonnierpublications.com"
    }
  ],
  "repositories": [
